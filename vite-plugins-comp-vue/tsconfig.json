{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "../../dist/client",
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client"],
    "paths": {
      "/@shared/*": ["shared/*"],
      "/@types/*": ["../../types/*"],
      "/@theme/*": ["theme-default/*"],
      "vitepress": ["index.ts"]
    }
  },
  "include": [
    ".",
    "../../types/shared.d.ts",
  ]
}
